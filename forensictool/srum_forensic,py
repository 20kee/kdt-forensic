import shutil

class srum:
    def __init__(self):
        pass

    def srum_copy(self, folder):
        to_file_path = folder+'/srum/'
        from_file_path = 'C:\\Windows\\System32\\sru'
        shutil.copytree(from_file_path, to_file_path)

def main(folder):
    srum_ = srum()
    srum_.srum_copy(folder)

if __name__ == '__main__':
    main('test')


